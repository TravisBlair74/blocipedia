<h1>Create New Collaboration</h1>
<div class="row">
  <div class="col-md-8">
    <%= form_for @collaboration do |f| %>
      <%= f.hidden_field :wiki_id, value: @wiki.id %>
      <div class="form-group">
        <%= f.label "User ID" %>
        <%= f.text_field :user_id, class: 'form-control', placeholder: "Enter Collaborator ID" %>
      </div>
      <div class="form-group">
        <%= f.submit "Save", class: 'btn btn-success' %>
      </div>
    <% end %>
  </div>
</div>
